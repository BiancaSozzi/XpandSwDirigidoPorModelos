<?xml version="1.0" encoding="UTF-8"?>
<utnSimilUML:Diagrama
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:utnSimilUML="http://www.example.org/utnSimilUML"
    xsi:schemaLocation="http://www.example.org/utnSimilUML utnSimilUML.ecore">
  <clase nombre="Persona">
    <operacion
        tipoRetorno="void"
        nombre="jugar">
      <parametros
          tipo="int"
          nombre="zapatillas"/>
      <parametros
          tipo="String"
          nombre="saltar"/>
    </operacion>
    <operacion
        tipoRetorno="void"
        nombre="comer"/>
    <operacion
        tipoRetorno="String"
        nombre="dormir">
      <parametros
          tipo="int"
          nombre="horas"/>
    </operacion>
    <atributo
        nombre="Apellido"
        tipo="String"/>
    <atributo
        nombre="edad"
        tipo="int"/>
  </clase>
</utnSimilUML:Diagrama>
